programa {
  funcao inicio() {
    inteiro num1, num2, multiplicacao,subtracao, divisao

    escreva("Digite um número: ")
    leia(num1)

    escreva("Digite outro número: ")
    leia(num2)

    multiplicacao=(num1 * num2)
    subtracao=(num1 - num2)
    divisao=(num1/num2)

    escreva("O resultado da divisão é: " + divisao + "O resultado da subtração é: " + subtracao + "O resultado da multiplicação é: " + multiplicacao)
    }
}
